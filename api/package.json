{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only --clear --inspect=0.0.0.0:31000 src/server",
    "dev:docker": "npm run typeorm:migrate && ts-node-dev --respawn --transpile-only --clear --poll --inspect=0.0.0.0:30000 src/server",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:coverage": "npm test -- --coverage",
    "test:unit": "npm run test -- -c jest-unit.config.js",
    "test:integration": "npm run test -- -c jest-integration.config.js",
    "generate:sql": "cd scripts && node ./txt-to-sql.js && node ./json-to-sql.js",
    "typeorm": "tsnd ./node_modules/typeorm/cli.js",
    "typeorm:migrate": "npm run typeorm -- migration:run"
  },
  "keywords": [],
  "author": {
    "name": "Leonardo Rocha",
    "email": "leonardo.rocha0803@gmail.com"
  },
  "license": "ISC",
  "devDependencies": {
    "@jest-mock/express": "^1.4.5",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.23",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "eslint": "^7.12.1",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.0.0",
    "husky": "^4.3.8",
    "jest": "^27.5.1",
    "jest-mock-extended": "^2.0.4",
    "lint-staged": "^12.3.7",
    "moment": "^2.29.1",
    "pg-mem": "^2.3.3",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cross-env": "^7.0.3",
    "express": "^4.17.3",
    "metadata": "^0.1.0",
    "module-alias": "^2.2.2",
    "pg": "^8.7.3",
    "reflect": "^0.1.3",
    "reflect-metadata": "^0.1.13",
    "serialize-error": "^8.1.0",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.29"
  }
}
